{
  "name": "mesbg",
  "version": "1.28.6",
  "homepage": "https://v2024.mesbg-list-builder.com/",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc -b && vite build",
    "postbuild": "node scripts/generate-sitemap.js",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run prettier && npm run lint",
    "check:fix": "npm run prettier:fix && npm run lint:fix",
    "version": "npm run version:win || npm run version:unix",
    "version:win": "sh changelog-version-update.sh %npm_package_version%",
    "version:unix": "./changelog-version-update.sh $npm_package_version",
    "postversion": "git push && git push --tags",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:data": "python3 data/scripts/main.py",
    "watch:data": "find ./data -name '*.xlsx' | entr npm run build:data                                                                                                                                                                   todo-app\n"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hello-pangea/dnd": "^17.0.0",
    "@mui/icons-material": "^6.0.2",
    "@mui/material": "^6.0.2",
    "chart.js": "^4.4.6",
    "fast-deep-equal": "^3.1.3",
    "file-save": "^0.2.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "npm": "^10.9.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.3.0",
    "react-numeric-input": "^2.2.3",
    "react-router-dom": "^7.0.1",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.3",
    "zipson": "^0.2.12",
    "zundo": "^2.2.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.11.1",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "playwright": "^1.53.2",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "sitemap": "^8.0.0",
    "storybook": "^9.0.16",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
